{
  "show_edit_predictions": false,
  "vim_mode": true,
  "telemetry": {
    "metrics": false
  },
  "base_keymap": "VSCode",
  "icon_theme": "Zed (Default)",
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "One Dark"
  },
  "agent": {
    "inline_assistant_model": {
      "provider": "zed.dev",
      "model": "claude-sonnet-4"
    },
    "always_allow_tool_actions": true,
    "default_profile": "write",
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-sonnet-4"
    }
  },
  "buffer_font_size": 14,
  "ui_font_size": 14,
  "buffer_font_family": "JetBrains Mono"
}
